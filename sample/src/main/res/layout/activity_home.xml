<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/btnChannels"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Channels" />

    <Button
        android:id="@+id/btnSocket"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Socket" />

    <Button
        android:id="@+id/btnTestChannelsApis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test Channels APIs" />

    <Button
        android:id="@+id/btnTestUsersApis"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Test Users APIs" />

    <Button
            android:id="@+id/btnGenericViewModel"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Generic ViewModel" />

</LinearLayout>